<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="56px"
        android:gravity="center"
        android:singleLine="true"
        android:text="@string/personal_str9"
        android:textColor="@color/colorEdit"
        android:textSize="60px"
        android:textStyle="bold" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="56px"
        android:background="#d6d6d6" />

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="160px"
        android:orientation="horizontal"
        android:paddingLeft="20px"
        android:paddingRight="20px">

        <TextView
            android:id="@+id/tv_cpd1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/personal_str17"
            android:textColor="@color/colorEdit"
            android:textSize="44px"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/et_cpd1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="20px"
            android:background="#00000000"
            android:gravity="center_vertical"
            android:inputType="textPassword"
            android:maxLength="50"
            android:singleLine="true"
            android:textColor="@color/colorEdit"
            android:textSize="44px" />
    </com.zhy.autolayout.AutoLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d6d6d6" />

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="160px"
        android:orientation="horizontal"
        android:paddingLeft="20px"
        android:paddingRight="20px">

        <TextView
            android:id="@+id/tv_cpd2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/personal_str18"
            android:textColor="@color/colorEdit"
            android:textSize="44px"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/et_cpd2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="20px"
            android:background="#00000000"
            android:gravity="center_vertical"
            android:inputType="textPassword"
            android:maxLength="50"
            android:singleLine="true"
            android:textColor="@color/colorEdit"
            android:textSize="44px" />
    </com.zhy.autolayout.AutoLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d6d6d6" />

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="160px"
        android:orientation="horizontal"
        android:paddingLeft="20px"
        android:paddingRight="20px">

        <TextView
            android:id="@+id/tv_cpd3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/personal_str19"
            android:textColor="@color/colorEdit"
            android:textSize="44px"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/et_cpd3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="20px"
            android:background="#00000000"
            android:inputType="textPassword"
            android:maxLength="50"
            android:singleLine="true"
            android:textColor="@color/colorEdit"
            android:textSize="44px" />
    </com.zhy.autolayout.AutoLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#d6d6d6" />

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="160px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_cpd_no"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:background="@drawable/fhe_click"
            android:gravity="center"
            android:text="@string/envir_str1"
            android:textColor="@color/colorEdit"
            android:textSize="50px"
            android:textStyle="bold" />

        <View
            android:layout_width="1px"
            android:layout_height="match_parent"
            android:background="#d6d6d6" />

        <TextView
            android:id="@+id/tv_cpd_ok"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:background="@drawable/fhe_click"
            android:gravity="center"
            android:text="@string/personal_str6"
            android:textColor="@color/colorEdit"
            android:textSize="50px"
            android:textStyle="bold" />
    </com.zhy.autolayout.AutoLinearLayout>

</com.zhy.autolayout.AutoLinearLayout>